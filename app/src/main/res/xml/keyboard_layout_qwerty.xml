<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:verticalGap="2dp"
    android:keyHeight="48dp">
    <Row>
        <Key android:codes="-10" android:keyLabel="Account List"
            android:keyEdgeFlags="left"
            android:horizontalGap="7.5%p" />
        <Key android:codes="-11" android:keyLabel="Account"
            android:horizontalGap="15%p"/>
        <Key android:codes="-12" android:keyLabel="Password"
            android:horizontalGap="15%p"/>
        <Key android:codes="-13" android:keyLabel="Additional Code"
            android:horizontalGap="15%p"/>
    </Row>

    <Row>
        <Key android:keyLabel="q" android:keyWidth="10%p"/>
        <Key android:keyLabel="w" android:keyWidth="10%p"/>
        <Key android:keyLabel="e" android:keyWidth="10%p"/>
        <Key android:keyLabel="r" android:keyWidth="10%p"/>
        <Key android:keyLabel="t" android:keyWidth="10%p"/>
        <Key android:keyLabel="y" android:keyWidth="10%p"/>
        <Key android:keyLabel="u" android:keyWidth="10%p"/>
        <Key android:keyLabel="i" android:keyWidth="10%p"/>
        <Key android:keyLabel="o" android:keyWidth="10%p"/>
        <Key android:keyLabel="p" android:keyWidth="10%p"/>
    </Row>

    <Row>
        <Key android:keyLabel="a" android:keyWidth="10%p" android:horizontalGap="5%p"/>
        <Key android:keyLabel="s" android:keyWidth="10%p" />
        <Key android:keyLabel="d" android:keyWidth="10%p" />
        <Key android:keyLabel="f" android:keyWidth="10%p" />
        <Key android:keyLabel="g" android:keyWidth="10%p" />
        <Key android:keyLabel="h" android:keyWidth="10%p" />
        <Key android:keyLabel="j" android:keyWidth="10%p" />
        <Key android:keyLabel="k" android:keyWidth="10%p" />
        <Key android:keyLabel="l" android:keyWidth="10%p" />
    </Row>

    <Row>
        <Key android:codes="-1"
            android:isModifier="true"
            android:keyIcon="@drawable/sym_keyboard_not_shift"
            android:isSticky="true"
            android:keyWidth="10%p"
            android:horizontalGap="5.5%p"
            android:keyEdgeFlags="left"/>
        <Key android:keyLabel="z" android:keyWidth="10%p" />
        <Key android:keyLabel="x" android:keyWidth="10%p" />
        <Key android:keyLabel="c" android:keyWidth="10%p" />
        <Key android:keyLabel="v" android:keyWidth="10%p" />
        <Key android:keyLabel="b" android:keyWidth="10%p" />
        <Key android:keyLabel="n" android:keyWidth="10%p" />
        <Key android:keyLabel="m" android:keyWidth="10%p" />
        <Key android:codes="-5"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:horizontalGap="3%p"
            android:keyWidth="10%p"
            android:isRepeatable="true"/>
    </Row>

    <!--<Row android:keyboardMode="@+id/mode_normal" android:rowEdgeFlags="bottom">-->
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-2" android:keyLabel="\?123"
            android:horizontalGap="6%p" android:keyWidth="10%p" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="," android:keyWidth="10%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space2"
            android:keyWidth="50%p"/>
        <Key android:keyLabel="." android:keyWidth="10%p"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return2"
            android:keyWidth="10%p" android:keyEdgeFlags="right" android:horizontalGap="2%p"/>
    </Row>
</Keyboard>