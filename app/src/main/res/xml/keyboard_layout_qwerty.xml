<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="4px"
    android:verticalGap="2dp"
    android:keyHeight="48dp">
    <Row>
        <Key android:codes="-10" android:keyLabel="Account List"
            android:keyEdgeFlags="left"
            android:horizontalGap="7.5%p" />
        <Key android:codes="-11" android:keyLabel="Account"
            android:horizontalGap="15%p"/>
        <Key android:codes="-12" android:keyLabel="Password"
            android:horizontalGap="15%p"/>
        <Key android:codes="-13" android:keyLabel="Additional Code"
            android:horizontalGap="15%p"/>
    </Row>

    <Row>
        <Key android:keyLabel="q"
            android:keyEdgeFlags="left"/>
        <Key android:keyLabel="w"/>
        <Key android:keyLabel="e"/>
        <Key android:keyLabel="r"/>
        <Key android:keyLabel="t"/>
        <Key android:keyLabel="y"/>
        <Key android:keyLabel="u"/>
        <Key android:keyLabel="i"/>
        <Key android:keyLabel="o"/>
        <Key android:keyLabel="p" android:keyEdgeFlags="right"/>
    </Row>

    <Row android:horizontalGap="1%p">
        <Key android:codes="1011" android:keyLabel="a" android:keyWidth="9%p"  android:keyEdgeFlags="left" />
        <Key android:codes="1012" android:keyLabel="s" android:keyWidth="9%p" />
        <Key android:codes="1013" android:keyLabel="d" android:keyWidth="9%p" />
        <Key android:codes="1014" android:keyLabel="f" android:keyWidth="9%p" />
        <Key android:codes="1015" android:keyLabel="g" android:keyWidth="9%p" />
        <Key android:codes="1016" android:keyLabel="h" android:keyWidth="9%p" />
        <Key android:codes="1017" android:keyLabel="j" android:keyWidth="9%p" />
        <Key android:codes="1018" android:keyLabel="k" android:keyWidth="9%p" />
        <Key android:codes="1019" android:keyLabel="l" android:keyWidth="9%p"  android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-1"
            android:isModifier="true"
            android:keyIcon="@drawable/sym_keyboard_not_shift"
            android:verticalGap="50%p"
            android:isSticky="true"
            android:horizontalGap="2%p"
            android:keyEdgeFlags="left"/>
        <Key android:keyLabel="z" android:keyWidth="9.5%p" />
        <Key android:keyLabel="x" android:keyWidth="9.5%p" />
        <Key android:keyLabel="c" android:keyWidth="9.5%p" />
        <Key android:keyLabel="v" android:keyWidth="9.5%p" />
        <Key android:keyLabel="b" android:keyWidth="9.5%p" />
        <Key android:keyLabel="n" android:keyWidth="9.5%p" />
        <Key android:keyLabel="m" android:keyWidth="9.5%p" />
        <Key android:codes="-5"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:horizontalGap="2%p"
            android:isRepeatable="true"/>
    </Row>

    <!--<Row android:keyboardMode="@+id/mode_normal" android:rowEdgeFlags="bottom">-->
    <Row android:rowEdgeFlags="bottom" >
        <Key android:codes="-2" android:keyLabel="\?123"
            android:horizontalGap="3%p" android:keyWidth="10%p" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="," android:keyWidth="10%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="50%p"/>
        <Key android:keyLabel="." android:keyWidth="10%p"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="10%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>